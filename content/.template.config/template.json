{
	"$schema": "http://json.schemastore.org/template",
	"author": "Juan Luis Guerrero Minero",
	"classifications": [ "ElGuerre", "Web", "WebAPI", "Swagger", "ApiRest" ],
	"name": "ElGuerre Api Rest",	
	"groupIdentity":"ElGuerre.ApiRest",
	"identity": "ElGuerre.ApiRest",	// Unique name for this template
	"shortName": "elguerre-apirest",	// Short name that can be used on the cli
	"tags": {
		"language": "C#",
		"type": "project"
	},
	"sourceName": "ElGuerre.Items",// Will replace the string 'ElGuerre.Items' with the value provided via -n.
	"preferNameDirectory": true,
	"symbols": {		
		"Title": {
		  "type": "parameter",
		  "datatype": "string",
		  "defaultValue": "Project Title",
		  "replaces": "PROJECT-TITLE",
		  "description": "The name of the project which determines the assembly product name."
		}
	}
}
